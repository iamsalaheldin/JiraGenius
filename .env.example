# Jira Configuration (OPTIONAL - users will enter these via the login UI)
# Leave these empty to force users to log in with their own credentials
JIRA_BASE_URL=
JIRA_EMAIL=
JIRA_API_TOKEN=

# LLM Configuration
# Options: gemini, openai, anthropic
LLM_PROVIDER=gemini

# LLM API Keys (set the one matching your LLM_PROVIDER)
GEMINI_API_KEY=your-gemini-api-key
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# Node Environment
NODE_ENV=development
